    <script>
        $(function() {
            $(".close").on('click', function() {
                $('#addDvice').addClass('hidden');
            });

            $(".T-RB-Button").on('click', function() {
                $('#addDvice').removeClass('hidden');
            });

            $(".close").on('click', function() {
                $('#tableForm').addClass('hidden');
            });

            $("#table").on('click', function() {
                $('#tableForm').removeClass('hidden');
            });
        })
    </script>
    <link href="../../css/T-c3.css" rel="stylesheet" type="text/css">
    <script src="../../js/d3.min.js" charset="utf-8"></script>
    <script src="../../js/c3.min.js" charset="utf-8"></script>
    <section class="wrapper">
        <div class="container">
            <div class="12u">
                <h3>Breath <i class="fa fa-table" aria-hidden="true"  id="table"></i></h3>
                <div id="WindChart"></div>
            </div>
        </div>
    </section>

    <div class="wrapper">
        <div class="container">
            <div class="T-PosterForm hidden">
                <form action="/T-CreateGroup" method="post" enctype='multipart/form-data'>
                    <div class="form">
                        <h2>Add Devices</h2>
                        <label>Device * <a href=""><i class="fa fa-info-circle fa-lg" aria-hidden="true"></i></a></label>
                        <input type="text" name="name" />
                        <label>Serial Number *</label>
                        <input type="text" name="name" />
                        <button class="post">Add</button>
                        <div class="close" title="close"></div>
                    </div>
                </form>
            </div>
        </div>
    </div>
    <div class="wrapper">
        <div class="container">
            <div class="T-PosterForm hidden" id="addDvice">
                <form action="/T-CreateGroup" method="post" enctype='multipart/form-data'>
                    <div class="form">
                        <h2>Add Devices</h2>
                        <label>Device * <a href=""><i class="fa fa-info-circle fa-lg" aria-hidden="true"></i></a></label>
                        <div class="select-wrapper">
                            <select name="category" id="category">
                        		<option value="">- Device -</option>
                        		<option value="1">Breath</option>
                        		<option value="1">Heartbeat</option>
                        		<option value="1">Mind</option>
                        	</select>
                        </div>
                        <label>Serial Number *</label>
                        <input type="text" name="name" />
                        <button class="post">Add</button>
                        <div class="close" title="close"></div>
                    </div>
                </form>
            </div>
            <div class="T-PosterForm hidden" id="tableForm">
                <div class="form">
                    <h3>Table</h3>
                    <div class="table-wrapper">
                        <table>
                            <thead>
                                <tr>
                                    <th>Time</th>
                                    <th>CO2</th>
                                    <th>O2</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td>12:00</td>
                                    <td>29.99</td>
                                    <td>29.99</td>
                                </tr>
                                <tr>
                                    <td>13:00</td>
                                    <td>29.99</td>
                                    <td>29.99</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                    <div class="close" title="close"></div>
                </div>
            </div>
        </div>
    </div>
    <button class="T-RB-Button"><i class="fa fa-plus" aria-hidden="true"></i></button>
    <script>
        $(document).ready(function() {
            var WindChart = c3.generate({
                bindto: '#WindChart',
                data: {
                    x: 'x',
                    columns: [
                        ['x', '0.89', '1.22', '1.56', '1.89', '2.22', '2.56', '2.89', '3.22', '3.55', '3.89', '4.22', '4.55', '4.89', '5.22', '5.55', '5.89', '6.22', '6.55', '6.89', '7.55'],
