                        ['O2', 11.22, 11.05, 11.18, 11.79, 12.08, 11.79, 12.10, 12.41, 11.51, 11.79, 12.38, 12.37, 12.06, 12.08, 12.10, 12.71, 12.10, 12.41, 12.12, 12.42, 11.83]
                    ],
                    type: 'area-spline'
                },
                axis: {
                    x: {
                        type: 'category',
                        categories: [],
                        tick: {
                            rotate: 75,
                            multiline: false
                        }
                    },
                    y: {
                        label: {
                            text: '',
                            position: 'outer-middle'
                        }
                    }
                },
                color: {
                    pattern: ['#3cba54', '#4885ed']
                },
                point: {
                    r: 3.5
                }
            });
        });
    </script>
